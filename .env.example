# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# YouTube API - Multiple Keys for Rotation (RECOMMENDED)
# Comma-separated list of API keys for automatic rotation on quota limits
YOUTUBE_API_KEYS=key1,key2,key3,key4

# YouTube API - Single Key (fallback if YOUTUBE_API_KEYS not set)
YOUTUBE_API_KEY=your_youtube_api_key

# Redis (Upstash) - for search caching (optional)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# Worldcoin MiniKit
NEXT_PUBLIC_APP_ID=app_your_worldcoin_app_id
